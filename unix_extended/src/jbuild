(library
 ((name unix_extended)
  (public_name shell.unix_extended)
  (libraries (core
              re2
              shell_internal
              spawn
              ))
  (preprocessor_deps (config.h))
  (c_names (fork_exec
            unix_extended_stubs))
  (preprocess (pps (ppx_jane ppxlib.runner)))))

(rule
 ((targets (config.h))
  (deps ())
  (action (bash "cp ${findlib:core_kernel.config_h:config.h} ${@}"))))

(jbuild_version 1)
